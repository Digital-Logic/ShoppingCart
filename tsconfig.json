{
    "compilerOptions": {
      "target": "es2018",
      "module": "esnext",
      "moduleResolution": "node",
      "lib": [
        "esnext",
        "esnext.asynciterable",
        "dom"
      ],
      "esModuleInterop": true,
      "allowJs": true,
      "sourceMap": true,
      "strict": true,
      "noEmit": true,
      "baseUrl": ".",
      "resolveJsonModule": true,
      "paths": {
        "~/*": [
          "./*"
        ],
        "@/*": [
          "./*"
        ]
      },
      "types": [
        "@nuxt/vue-app",
        "@nuxtjs/axios",
        "@types/node",
        "@nuxt/types"
      ]
    },
    "include": [
        "**/*.ts", "**/*.vue"
    ],
    "files": [
        "vue-shim.d.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }